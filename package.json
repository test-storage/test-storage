{
  "name": "test-storage",
  "version": "0.42.0",
  "description": "Test Case Management System",
  "license": "GPLv3",
  "author": "Dmitry Zakharov",
  "private": true,
  "keywords": [
    "test management",
    "test case management"
  ],
  "bugs": {
    "url": "https://github.com/pumano/test-storage/issues"
  },
  "scripts": {
    "ng": "nx",
    "lint": "nx workspace-lint && ng lint",
    "build": "ng build --configuration production",
    "test": "jest",
    "serve": "nx serve --hmr",
    "serve:all": "nx run-many --target=serve --projects=api,test-storage --parallel",
    "clean": "rimraf node_modules package-lock.json",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "extract": "ngx-translate-extract --input ./src --output ./i18n/*.json --clean --sort --format namespaced-json",
    "nx": "nx",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "update:check": "ng update",
    "dep-graph": "nx dep-graph",
    "workspace-schematic": "nx workspace-schematic",
    "help": "nx help",
    "postinstall": "node ./decorate-angular-cli.js"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint && npm test"
    }
  },
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.0.0"
  },
  "dependencies": {
    "@angular/animations": "12.2.7",
    "@angular/common": "12.2.7",
    "@angular/compiler": "12.2.7",
    "@angular/compiler-cli": "12.2.7",
    "@angular/core": "12.2.7",
    "@angular/forms": "12.2.7",
    "@angular/platform-browser": "12.2.7",
    "@angular/platform-browser-dynamic": "12.2.7",
    "@angular/pwa": "^0.1102.11",
    "@angular/router": "12.2.7",
    "@angular/service-worker": "12.2.7",
    "@auth0/angular-jwt": "^5.0.2",
    "@cds/core": "^5.0.3",
    "@clr/angular": "^5.0.3",
    "@clr/icons": "^5.0.3",
    "@clr/ui": "^5.0.3",
    "@nestjs/common": "^7.6.15",
    "@nestjs/core": "^7.6.15",
    "@nestjs/platform-express": "^7.6.15",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@webcomponents/webcomponentsjs": "^2.5.0",
    "angular2-notifications": "^9.0.0",
    "core-js": "^3.11.1",
    "font-awesome": "^4.7.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.3",
    "socket.io-client": "^2.3.1",
    "tailwindcss": "^2.2.16",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.2.7",
    "@angular-devkit/schematics": "^11.2.11",
    "@angular-eslint/builder": "2.0.0",
    "@angular-eslint/eslint-plugin": "12.3.1",
    "@angular-eslint/eslint-plugin-template": "12.3.1",
    "@angular-eslint/schematics": "2.0.0",
    "@angular-eslint/template-parser": "12.3.1",
    "@angular/cli": "12.2.7",
    "@angular/language-service": "12.2.7",
    "@babel/preset-env": "^7.13.10",
    "@biesbjerg/ngx-translate-extract": "^7.0.4",
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@nestjs/schematics": "^7.3.0",
    "@nestjs/testing": "^7.6.14",
    "@ngneat/tailwind": "^6.0.3",
    "@nrwl/angular": "12.9.0",
    "@nrwl/cypress": "12.9.0",
    "@nrwl/eslint-plugin-nx": "12.9.0",
    "@nrwl/jest": "12.9.0",
    "@nrwl/linter": "12.9.0",
    "@nrwl/nest": "12.9.0",
    "@nrwl/node": "12.9.0",
    "@nrwl/workspace": "12.9.0",
    "@types/jest": "^26.0.20",
    "@types/socket.io-client": "^1.4.35",
    "@typescript-eslint/eslint-plugin": "4.28.5",
