    "faker": "^5.5.3",
    "husky": "^4.3.8",
    "jest": "27.0.3",
    "jest-preset-angular": "9.0.7",
    "postcss": "^8.2.8",
    "postcss-import": "^14.0.0",
    "postcss-loader": "^5.2.0",
    "prettier": "2.4.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.3",
    "ts-jest": "27.0.3",
    "ts-loader": "^8.0.18",
    "ts-node": "^9.1.1",
    "typescript": "4.3.5"
  }
}
